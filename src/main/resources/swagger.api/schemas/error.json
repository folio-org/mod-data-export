{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Error",
  "properties": {
    "message": {
      "description": "Error message",
      "type": "string"
    },
    "type": {
      "description": "Error type",
      "type": "string"
    },
    "code": {
      "description": "Error code",
