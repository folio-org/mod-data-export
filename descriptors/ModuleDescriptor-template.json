{
  "id": "${artifactId}-${version}",
  "name": "Data Export Module",
  "requires": [],
  "provides": [
    {
      "id": "data-export",
      "version": "1.0",
      "handlers": [
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/data-export/export",
          "permissionsRequired": [
            "data-export.export"
          ],
          "modulePermissions": [
          ]
        },
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/data-export/fileDefinitions/{fileDefinitionId}",
          "permissionsRequired": [
            "data-export.filedefinitions.get"
          ],
          "modulePermissions": [
          ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/data-export/fileDefinitions/{fileDefinitionId}",
          "permissionsRequired": [
            "data-export.filedefinitions.uploadfile"
          ],
          "modulePermissions": [
          ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/data-export/fileDefinitions",
          "permissionsRequired": [
            "data-export.filedefinitions.post"
          ],
          "modulePermissions": [
          ]
        }
      ]
    },
    {
      "id": "_tenant",
      "version": "1.2",
      "interfaceType": "system",
      "handlers": [
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/_/tenant"
        },
        {
          "methods": [
            "DELETE"
          ],
          "pathPattern": "/_/tenant"
        }
      ]
    }
  ],
  "permissionSets": [],
  "launchDescriptor": {
    "dockerImage": "${artifactId}:${version}",
    "dockerPull": false,
    "dockerArgs": {
      "HostConfig": {
        "Memory": 542293850,
        "PortBindings": { "8081/tcp": [ { "HostPort": "%p" } ] }
      }
    },
    "env": [
      { "name": "JAVA_OPTIONS",
        "value": "-XX:MaxRAMPercentage=66.0"
      },
      { "name": "DB_HOST", "value": "postgres" },
      { "name": "DB_PORT", "value": "5432" },
      { "name": "DB_USERNAME", "value": "folio_admin" },
      { "name": "DB_PASSWORD", "value": "folio_admin" },
      { "name": "DB_DATABASE", "value": "okapi_modules" },
      { "name": "DB_QUERYTIMEOUT", "value": "60000" },
      { "name": "DB_CHARSET", "value": "UTF-8" },
      { "name": "DB_MAXPOOLSIZE", "value": "5" }
    ]
  }
}
